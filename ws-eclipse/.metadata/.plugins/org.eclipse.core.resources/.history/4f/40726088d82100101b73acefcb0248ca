import java.util.Locale;

public class treinoVariaveis {

	public static void main(String[] args) {
		
		Locale.setDefault(Locale.US);
		
		int idade ;
		idade = 25;
		System.out.println(idade);
		
		int valor1;
		int valor2;
		valor1 = 150;
		valor2 = 150;
		
		System.out.println("o resultado " + (valor1 + valor2));
		
	  
	}

}

import java.util.Locale;

public class ProcessamentoCasting {

	public static void main(String[] args) {
		
		Locale.setDefault(Locale.US);
		
		//Entrada de dados 
		int a , b ,  resultado;
		a = 5;
		b = 2;
		
		//Processamento de dados
		resultado = a / b;
		
		System.out.println(resultado);
			
	}

}
